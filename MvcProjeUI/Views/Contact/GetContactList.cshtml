@using EntityLayer.Concrete
@model List<Contact>
@{
    ViewBag.Title = "GetContactList";
}

<h2>İletişim Listeleme</h2>

<table class="table table-hover">
    <tr>
        <td>Contact Id</td>
        <td>Kullanıcı Adı</td>
        <td>Kullanıcı Email</td>
        <td>Konu</td>
        <td>Mesaj</td>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.ContactId</td>
    <td>@item.UserName</td>
    <td>@item.UserMail</td>
    <td>@item.Subject</td>
    <td>@item.Message</td>

</tr>
    }
</table>

<a class="btn btn-info" href="/Contact/AddContact">Yeni İletişim</a>
